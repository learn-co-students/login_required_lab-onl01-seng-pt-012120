<!DOCTYPE html>
<html>
<head>
  <title>LoginRequiredLab</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>
<% if session[:name] %>
	<%= form_tag(:action => "destroy", :method => "post") do %>
		<%= submit_tag "Logout" %>
	<% end -%>
<% end %>
</body>
</html>
